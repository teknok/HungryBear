<!doctype html>
<html lang="en">

<head>
  <title>
    <%=data.name%>
  </title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <link href="https://fonts.googleapis.com/css?family=Playfair+Display:300,400,700,800|Open+Sans:300,400,700" rel="stylesheet">

  <link rel="stylesheet" href="/css/bootstrap.css">
  <link rel="stylesheet" href="/css/animate.css">
  <link rel="stylesheet" href="/css/owl.carousel.min.css">

  <link rel="stylesheet" href="/css/magnific-popup.css">
  <link rel="stylesheet" href="/css/aos.css">

  <link rel="stylesheet" href="/css/bootstrap-datepicker.css">
  <link rel="stylesheet" href="/css/jquery.timepicker.css">

  <link rel="stylesheet" href="/fonts/ionicons/css/ionicons.min.css">
  <link rel="stylesheet" href="/fonts/fontawesome/css/font-awesome.min.css">

  <link rel="stylesheet" href="/fonts/flaticon/font/flaticon.css">

  <!-- Theme Style -->
  <link rel="stylesheet" href="/css/style.css">
  <script type="text/javascript" src="https://static.sketchfab.com/api/sketchfab-viewer-1.3.0.js"></script>
  <script src="/js/jquery-3.2.1.min.js"></script>


  <body>


    <div class="">

      <header class="site-header" style="background-color: rgb(78, 78, 78)">
        <div class="row align-items-center">
          <div class="col-5 col-md-3">
            <ul class="list-unstyled social">
              <li>
                <a href="#">
                  <span class="fa fa-facebook"></span>
                </a>
              </li>
              <li>
                <a href="#">
                  <span class="fa fa-twitter"></span>
                </a>
              </li>
              <li>
                <a href="#">
                  <span class="fa fa-instagram"></span>
                </a>
              </li>
            </ul>
          </div>
          <div class="col-2 col-md-6 text-center">
            <a href="index.html" class="site-logo">HB</a>
          </div>
          <div class="col-5 col-md-3 text-right menu-burger-wrap"></div>
        </div>

      </header>
      <!-- site-header -->

      <div class="main-wrap">
        <div class="cover_1 cover_sm">
          <div class="img_bg" style="background-image: url(<%=data.image%>);" data-stellar-background-ratio="0.7">
            <div class="container">
              <div class="row align-items-center justify-content-center">
                <div class="col-md-7" data-aos="fade-up">
                  <h2 class="heading">
                    <%=data.name%>'s Menu</h2>
                  <p class="lead"></p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- .cover_1 -->

        <div class="section">
          <div class="container">
            <div class="row justify-content-center">
              <div class="col-md-12" data-aos="fade-up">
                <h2 class="mb-5 text-center">Click here to see the ambience of
                  <%=data.name%>
                </h2>
                <iframe width="100%" height="400" allowfullscreen autoLoad style="border-style:none;" src="https://cdn.pannellum.org/2.4/pannellum.htm?panorama=https://pannellum.org/images/alma.jpg"></iframe>
                <h2 class="mb-5 text-center">Menu List with Price</h2>
                <div class="row">
                  <div class="col-3" style="padding-top: 50px">
                    <% _.forEach(data.types,function(val){ %>
                      <ul>
                        <li>
                          <a href="#" style="text-decoration: none; color: black"><%=val.name%></a>
                        </li>
                      </ul>
                      <%})%>
                  </div>
                  <div class="col-5">

                    <% _.forEach(data.types,function(val){ %>
                      <h3 class="text-center" style="color: #32CD32; padding-bottom:20px  ">
                        <%=val.name%>
                      </h3>
                      <div class="tab-content" id="pills-tabContent">
                        <div class="fade show active" id="pills-<%=val.name%>" role="tabpanel" aria-labelledby="pills-breakfast-tab">
                          <% _.forEach(data.menu,function(val1){ %>
                            <%if(val1.type==val.name){%>
                              <div class="d-block d-md-flex menu-food-item">
                                <div class="text order-1 mb-3">
                                  <h3>
                                    <input type="hidden" style="display:none;" id="uid" value="<%=val1.SFBUID%>">
                                    <a href="/city/<%=data.city%>/<%=data.username%>/<%=val1.SFBUID%>">
                                      <%=val1.name%>
                                    </a>
                                  </h3>
                                  <p>
                                    <%=val1.SFBUID%>
                                  </p>
                                </div>
                                <div class="price order-2">
                                  <strong>
                                    <%=val1.price%>
                                  </strong>

                                </div>
                              </div>
                              <% } }) %>
                        </div>
                        
                      </div>
                      <% }) %>
                    </div>
                    <div class="col-4" style="background-image: url(/img/Cart_empty_-_menu_2x_ejjkf2.webp);height: 350px">
                      <h3 class="text-center">Cart</h3>
                      
                    </div>
                </div>


              </div>
            </div>
          </div>
        </div>



      </div>
      <hr>
      <footer style="color: black">
        <div class="container">
          <div class="row">
            <div class="col col--lg-4 col--md-6 u-gap-bottom@md-down u-text-center@sm-down">
              <div class="u-gap-bottom">
                <img src="/assets/img/kabaq-logo.svg" width="150" height="150" alt="">
              </div>
              <div class="u-font-size-xs">
                <a href="https://www.theglimpsegroup.com/" class="u-link-white" target="_blank">a Teknok Group Company Â© 2017</a>
              </div>
            </div>
            <div class="col col--lg-2 col--xs-6 u-gap-bottom@md-down">
              <span class="c-label u-block u-gap-bottom-medium@lg-up u-gap-bottom-small@md-down">CONTACT</span>
              <div class="u-font-size-sm u-gap-bottom-small">
                <p class="u-clear-gap">
                  70 W 40th Street,
                  <br/> 16th Floor,
                  <br/> New York, NY, 10018
                </p>
                <p class="u-clear-gap">hello@hungrybear.in</p>
              </div>
              <div class="u-flex">

                <a href="https://www.facebook.com/kabaqapp/" class="c-social u-gap-right-small" title="Facebook" target="_blank">
                  <svg class="icon">
                    <use xlink:href="#icon-facebook"></use>
                  </svg>
                </a>
                <a href="https://www.instagram.com/kabaqapp/" class="c-social u-gap-right-small" title="Instagram" target="_blank">
                  <svg class="icon">
                    <use xlink:href="#icon-instagram"></use>
                  </svg>
                </a>
                <a href="https://twitter.com/kabaqApp" class="c-social " title="Twitter" target="_blank">
                  <svg class="icon">
                    <use xlink:href="#icon-twitter"></use>
                  </svg>
                </a>

              </div>
            </div>
            <div class="col col--lg-2 col--sm-6 u-gap-bottom@md-down u-hidden@sm-down">
              <span class="c-label u-block u-gap-bottom-medium@lg-up u-gap-bottom-small@md-down">ABOUT</span>
              <div class="u-font-size-sm">
                <a href="#" target="_blank" class="u-block u-link-white">Watch the Video</a>
                <a href="#" class="u-block u-link-white">Get In Touch</a>
                <a href="#" class="u-block u-link-white">FAQ</a>
              </div>
            </div>

            <div class="col col--lg-2 col--xs-6">
              <span class="c-label u-block u-gap-bottom-medium@lg-up u-gap-bottom-small@md-down">DOWNLOAD</span>

              <a href="#" class="u-inline-block u-gap-bottom-small" target="_blank">
                <img src="/assets/img/apple-store-white.svg" width="140" height="50" alt="">
              </a>
            </div>
          </div>
        </div>
      </footer>
      <script type="text/javascript">
      </script>
</head>
<script src="/js/jquery-migrate-3.0.1.min.js"></script>
<script src="/js/popper.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/owl.carousel.min.js"></script>
<script src="/js/jquery.waypoints.min.js"></script>

<script src="/js/bootstrap-datepicker.js"></script>
<script src="/js/jquery.timepicker.min.js"></script>
<script src="/js/jquery.stellar.min.js"></script>

<script src="/js/aos.js"></script>


<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>

<script src="/js/main.js"></script>

</body>

</html>